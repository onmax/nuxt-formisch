{
  "name": "nuxt-formisch",
  "version": "0.0.5",
  "description": "Formisch integration for Nuxt - schema-based, headless form library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nuxt-formisch.git"
  },
  "homepage": "https://nuxt-formisch.onmax.me",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bumpp && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@formisch/vue": "^0.6.5",
    "@nuxt/kit": "^4.2.0",
    "@standard-schema/spec": "^1.1.0",
    "scule": "^1.3.0",
    "tailwind-variants": "^3.2.2"
  },
  "peerDependencies": {
    "@nuxt/ui": ">=3.0.0",
    "valibot": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/ui": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/devtools": "catalog:dx",
    "@nuxt/eslint-config": "catalog:lint",
    "@nuxt/module-builder": "catalog:build",
    "@nuxt/schema": "catalog:backend",
    "@nuxt/test-utils": "catalog:test",
    "@types/node": "catalog:build",
    "bumpp": "catalog:dx",
    "eslint": "catalog:lint",
    "nuxt": "catalog:frontend",
    "typescript": "catalog:build",
    "valibot": "catalog:inlined",
    "vitest": "catalog:test",
    "vue-tsc": "catalog:build"
  }
}
